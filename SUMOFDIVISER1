class sumofdiviser1
{
	public static void main(String[] args)
	{
		int n=28;
		int a=1;
		int sum=0;
		while(a<=n/2)//1<=14
		{
			if(n%a==0)
			{
				sum=sum+a;
			}
			a++;
		}
		if(sum==n)
		{
			System.out.println("perfect number");
		}
		else{
			System.out.println("not a perfect number");
		}
	}
}
